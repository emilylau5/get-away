<script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>


<div class="container">
  <div class="panel panel-info">
    <div class="panel-body">
      <form>
        <div class="form-group">
          <label for="username-input">Username</label>
          <div class="row">
            <div class="col-xs-10">
            <input class="form-control" id="username-input" type="text">
            {{user}}
            </div>
            <div class="col-xs-2">
            <a href="#">Change</a>
            </div>
          </div>
        </div>
        <div class="form-group">
          <label for="email-input">Email</label>
          <div class="row">
            <div class="col-xs-10">
            <input class="form-control" id="email-input" type="text">
            {{email}}
            </div>
            <div class="col-xs-2">
            <a href="#">Change</a>
            </div>
          </div>
        </div>
        <div class="form-group">
          <label for="password-input">Password</label>
          <div class="row">
            <div class="col-xs-10">
            <input class="form-control" id="password-input" type="text">
            {{password}}
            </div>
            <div class="col-xs-2">
            <a href="#">Change</a>
            </div>
          </div>
        </div>
        <div class="row">
          <div class="col-xs-12">
        <button class="btn btn-primary" id="add-user-btn" type="submit">Submit</button>
        </div>
      </div>
      </form>
    </div>
  </div>

<!-- ********************************* -->
</div>









</div>

<script type="text/javascript">
  if(!Cookies2.get("UserID")) {
    window.location.href = "/";
  }
</script>
<script src="assets/js/bookmark.js" type="text/javascript"></script>